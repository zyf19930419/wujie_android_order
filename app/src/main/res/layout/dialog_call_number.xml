<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/white"
    android:orientation="vertical">

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:paddingBottom="13dp"
        android:paddingTop="13dp"
        android:text="提醒"
        android:textColor="@color/f333333"
        android:textSize="12sp" />

    <include layout="@layout/view_hor" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:paddingBottom="32dp"
        android:paddingTop="34dp"
        android:text="叫号无人应答，确定过号。"
        android:textColor="@color/f333333"
        android:textSize="14sp" />

    <include layout="@layout/view_hor" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/dialog_disimis"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center"
            android:padding="12dp"
            android:text="取消"
            android:textColor="@color/f666666"
            android:textSize="13sp" />

        <include layout="@layout/view_ver" />

        <TextView
            android:id="@+id/dialog_too_number"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:padding="12dp"
            android:gravity="center"
            android:text="确定过号"
            android:textColor="@color/f666666"
            android:textSize="13sp" />
    </LinearLayout>
</LinearLayout>